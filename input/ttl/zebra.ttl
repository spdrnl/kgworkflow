# File source https://folk.universitetetioslo.no/martige/what/20120422/zebra.html

<http://folk.uio.no/martige/what/2012/04/22/zebra> a owl:Ontology .

[ a owl:AllDisjointClasses ; owl:members ( :Color :Drink :House :Person :Pet :Smoke ) ] .

:Color a owl:Class ;
    owl:equivalentClass [ a owl:Class ; owl:oneOf ( :Blue :Yellow :Ivory :Green :Red ) ] .

:Drink a owl:Class ;
    owl:equivalentClass [ a owl:Class ; owl:oneOf ( :OrangeJuice :Water :Tea :Milk :Coffee ) ] .

:Pet a owl:Class ;
    owl:equivalentClass [ a owl:Class ; owl:oneOf ( :Horse :Zebra :Fox :Dog :Snails ) ] .

:Smoke a owl:Class ;
    owl:equivalentClass [ a owl:Class ; owl:oneOf ( :Kools :Chesterfield :LuckyStrike :OldGold :Parliament ) ] .

:House a owl:Class ;
    owl:equivalentClass [ a owl:Class ; owl:oneOf ( :House5 :House4 :House2 :House3 :House1 ) ] .

:Person a owl:Class ;
    owl:equivalentClass [ a owl:Class ; owl:oneOf ( :Norwegian :Spaniard :Ukrainian :Japanese :Englishman ) ] .

:drinks a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:ObjectProperty ;
    rdfs:range :Drink ;
    rdfs:domain :Person .

:hasColor a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:ObjectProperty ;
    rdfs:range :Color ;
    rdfs:domain :House .

:hasPet a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Pet .

:livesIn a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:ObjectProperty ;
    rdfs:range :House ;
    rdfs:domain :Person .

:smokes a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Smoke .

:Person a owl:Class ;
    rdfs:subClassOf [ a owl:Class ; owl:intersectionOf ( [ a owl:Restriction ; owl:onProperty :drinks ; owl:someValuesFrom owl:Thing ] [ a owl:Restriction ; owl:onProperty :hasPet ; owl:someValuesFrom owl:Thing ] [ a owl:Restriction ; owl:onProperty :livesIn ; owl:someValuesFrom owl:Thing ] [ a owl:Restriction ; owl:onProperty :smokes ; owl:someValuesFrom owl:Thing ] ) ] .

:House a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ; owl:onProperty :hasColor ; owl:someValuesFrom owl:Thing ] .

:House1 :isLeftTo :House2 .

:House2 :isLeftTo :House3 .

:House3 :isLeftTo :House4 .

:House4 :isLeftTo :House5 .

[ a owl:NegativePropertyAssertion ; owl:assertionProperty :isNextTo ; owl:targetIndividual :House1 ; owl:sourceIndividual :House5 ] .

[ a owl:AllDifferent ; owl:distinctMembers ( :House2 :House1 :House4 :House3 :House5 ) ] .

:isNextTo a owl:IrreflexiveProperty , owl:ObjectProperty , owl:SymmetricProperty ;
    rdfs:domain :House ;
    rdfs:range :House .

:isRightTo a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:ObjectProperty ;
    owl:inverseOf :isLeftTo ;
    rdfs:subPropertyOf :isNextTo .

:isLeftTo a owl:ObjectProperty ;
    rdfs:subPropertyOf :isNextTo .

:House a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ; owl:onProperty :isNextTo ; owl:onClass :House ; owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ] , [ a owl:Restriction ; owl:onProperty :hasColor ; owl:someValuesFrom owl:Thing ] .

[ a owl:AllDifferent ; owl:distinctMembers ( :Green :Ivory :Red :Yellow :Blue ) ] .

[ a owl:AllDifferent ; owl:distinctMembers ( :Spaniard :Norwegian :Englishman :Ukrainian :Japanese ) ] .

[ a owl:AllDifferent ; owl:distinctMembers ( :Tea :OrangeJuice :Water :Coffee :Milk ) ] .

[ a owl:AllDifferent ; owl:distinctMembers ( :Dog :Zebra :Fox :Snails :Horse ) ] .

